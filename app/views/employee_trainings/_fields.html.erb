<%= render partial: 'shared/errors', locals: {object: f.object} %>

  <div class="form-group">
    <%= f.label :site_employee_id %><br>
    <%= f.collection_select :site_employee_id, SiteEmployee.all, :id, :to_s, class: "form-control"%>
  </div>
  <div class="form-group">
    <%= f.label :training_id %><br>
    <%= f.collection_select :training_id, Training.all, :id, :course_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :expiration_date %><br>
    <%= f.datetime_select :expiration_date, class: "form-control" %>
  </div>
  <br/>
  <div class="checkbox">
    <%= f.label :obtained? %><br>
    <%= f.check_box :obtained? %>
  </div>